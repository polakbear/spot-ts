    "@typescript-eslint/parser": "^5.13.0",
    "chai": "^4.3.6",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-chai-expect": "^3.0.0",
